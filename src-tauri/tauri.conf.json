{
  "productName": "Ease MD",
  "version": "0.1.0",
  "identifier": "com.ease.md",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": [
      "../dist/index.html",
      "../dist/main.js",
      "../dist/main.css",
      "../dist/main.js.map",
      "../dist/main.css.map"
    ]
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Untitled - Ease MD",
        "minWidth": 484,
        "width": 960,
        "height": 640,
        "url": "tauri://localhost/index.html",
        "devtools": true
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["main-capability"]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ]
  }
}
